{
	"title": "Hyper Tab",
	"rules": [
		{
			"description": "Tab if no keychord, else Hyper",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "tab",
						"modifiers": {
							"optional": [
								"any"
							]
						}
					},
					"to": [
						{
							"key_code": "right_shift",
							"modifiers": [
								"right_command",
								"right_option"
							],
							"lazy": true
						}
					],
					"to_if_alone": [
						{
							"key_code": "tab"
						}
					]
				}
			]
		}
	]
}
